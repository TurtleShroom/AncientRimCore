<?xml version="1.0" encoding="utf-8" ?>

<Patch>
	<Operation Class="PatchOperationConditional">
	<!-- Does this exist? -->
		<xpath>/Defs/ResearchProjectDef[defName="TreeSowing"]</xpath>
		<!-- If it does, add this. -->
		<match Class="PatchOperationAdd">
			<xpath>/Defs/ResearchProjectDef[defName="TreeSowing"]/tags</xpath>
			<value>
				<li>TribalStart</li>
				<li>GaulStart</li>
				<li>RomanStart</li>
				<li MayRequire="oskarpotocki.vfe.classical">ClassicalStart</li>
				<li MayRequire="oskarpotocki.vanillafactionsexpanded.medievalmodule">VFEM_MedievalStart</li>
				<li MayRequire="morishep.tarojun.onioftherim">OniStart</li>
			</value>
		</match>
		<!-- If the first XPATH does not exist, the original code stands. -->
	</Operation>
	
